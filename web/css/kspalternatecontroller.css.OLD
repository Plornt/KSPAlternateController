	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#112359), color-stop(100%,#19182d)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #112359 0%,#19182d 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #112359 0%,#19182d 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #112359 0%,#19182d 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #112359 0%,#19182d 100%); /* W3C */

	-webkit-border-radius: 0px 0px 20px 20px;
	border-radius: 0px 0px 20px 20px;
}
