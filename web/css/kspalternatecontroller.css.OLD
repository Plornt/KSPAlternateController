	background: -webkit-linear-gradient(top,  #393b60 0%,#274677 48%,#192f5e 52%,#151f49 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #393b60 0%,#274677 48%,#192f5e 52%,#151f49 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #393b60 0%,#274677 48%,#192f5e 52%,#151f49 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #393b60 0%,#274677 48%,#192f5e 52%,#151f49 100%); /* W3C */
